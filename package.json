{
  "name": "figma-variable-fonts",
  "version": "1.0.0",
  "description": "Figma plugin for rendering variable fonts",
  "scripts": {
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "start": "webpack-dev-server",
    "test": "jest",
    "plugin:build": "webpack --mode=production --config webpack.figma.config.js",
    "clean": "rd /s/q built",
    "plugin:build:watch": "webpack --mode=development --config webpack.figma.config.js --watch"
  },
  "author": "Leonard Bogdonoff",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rememberlenny/figma-variable-fonts"
  },
  "keywords": [
    "variable-fonts",
    "figma",
    "figma-plugins"
  ],
  "dependencies": {
    "@figma/plugin-typings": "^1.15.0",
    "@types/react-rangeslider": "^2.2.2",
    "antd": "^4.2.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-rangeslider": "^2.2.0",
    "react-redux": "^7.2.0",
    "react-router": "5.1.2",
    "react-router-config": "5.1.1",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "samsa": "git+https://github.com/rememberlenny/samsa.git#master",
    "styled-components": "^5.1.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.13.5",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-redux": "^7.1.8",
    "@types/react-router": "^5.1.7",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/styled-components": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsdoc": "^24.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "jest": "^25.5.4",
    "lint-staged": "^10.2.9",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.0",
    "redux-devtools-extension": "^2.13.8",
    "source-map-loader": "^1.0.0",
    "style-loader": "^1.2.1",
    "ts-import-plugin": "^1.6.6",
    "ts-jest": "^25.5.0",
    "ts-loader": "^7.0.3",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,css,json}": [
      "prettier --write",
      "git add"
    ]
  }
}
